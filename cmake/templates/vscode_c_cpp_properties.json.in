{
    "version": 4,
    "configurations": [
        {
            "name": "@PROJECT_NAME@",
            "includePath": [
@INCLUDE_PATHS_JSON@
            ],
            "defines": [
@DEFINES_JSON@
            ],
            "compilerPath": "@GCC_PATH@",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "@INTELLISENSE_MODE@",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerArgs": [
                "-mcpu=@TARGET_ARCH@",
                "-mthumb",
                "-mfpu=fpv4-sp-d16",
                "-mfloat-abi=hard",
                "-fdata-sections",
                "-ffunction-sections",
                "-Wall",
                "-fstack-usage",
                "--specs=nano.specs"
            ],
            "browse": {
                "path": [
@INCLUDE_PATHS_JSON@
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        }
    ]
}